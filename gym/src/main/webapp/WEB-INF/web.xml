<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>Gym</display-name>
	<welcome-file-list>
		<welcome-file>gym.html</welcome-file>
	</welcome-file-list>

	<servlet>
	<servlet-name>gymServlet</servlet-name>
	<servlet-class>com.xworkz.gym.servlet.GymServelt</servlet-class>
	<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>gymServlet</servlet-name>
	<url-pattern>/save</url-pattern>
	</servlet-mapping>
	
</web-app>